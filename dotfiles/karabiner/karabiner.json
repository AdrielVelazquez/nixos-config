{
  "global": {
    "check_for_updates_on_startup": false,
    "show_in_menu_bar": true,
    "show_profile_name_in_menu_bar": false
  },
  "profiles": [
    {
      "name": "Colemak-DH with Home Row Mods",
      "selected": true,
      "parameters": {
        "basic.to_if_alone_timeout_milliseconds": 300,
        "basic.to_if_held_down_threshold_milliseconds": 300
      },
      "simple_modifications": [
        { "from": { "key_code": "e" }, "to": [{ "key_code": "f" }] },
        { "from": { "key_code": "r" }, "to": [{ "key_code": "p" }] },
        { "from": { "key_code": "t" }, "to": [{ "key_code": "b" }] },
        { "from": { "key_code": "y" }, "to": [{ "key_code": "j" }] },
        { "from": { "key_code": "u" }, "to": [{ "key_code": "l" }] },
        { "from": { "key_code": "i" }, "to": [{ "key_code": "u" }] },
        { "from": { "key_code": "o" }, "to": [{ "key_code": "y" }] },
        { "from": { "key_code": "p" }, "to": [{ "key_code": "semicolon" }] },
        { "from": { "key_code": "g" }, "to": [{ "key_code": "g" }] },
        { "from": { "key_code": "h" }, "to": [{ "key_code": "m" }] },
        { "from": { "key_code": "v" }, "to": [{ "key_code": "d" }] },
        { "from": { "key_code": "b" }, "to": [{ "key_code": "v" }] },
        { "from": { "key_code": "n" }, "to": [{ "key_code": "k" }] },
        { "from": { "key_code": "m" }, "to": [{ "key_code": "h" }] }
      ],
      "complex_modifications": {
        "parameters": {
          "basic.to_if_alone_timeout_milliseconds": 300,
          "basic.to_if_held_down_threshold_milliseconds": 300,
          "basic.simultaneous_threshold_milliseconds": 50
        },
        "rules": [
          {
            "description": "Caps Lock: Tap = Escape, Hold = Hyphen",
            "manipulators": [
              {
                "type": "basic",
                "from": { "key_code": "caps_lock", "modifiers": { "optional": ["any"] } },
                "to_if_alone": [{ "key_code": "escape" }],
                "to_if_held_down": [{ "key_code": "hyphen" }]
              }
            ]
          },
          {
            "description": "Space: Tap = Space, Hold = Nav Layer (HJKL arrows)",
            "manipulators": [
              {
                "type": "basic",
                "from": { "key_code": "spacebar", "modifiers": { "optional": ["any"] } },
                "to_if_alone": [{ "key_code": "spacebar" }],
                "to_if_held_down": [{ "set_variable": { "name": "nav_layer", "value": 1 } }],
                "to_after_key_up": [{ "set_variable": { "name": "nav_layer", "value": 0 } }]
              }
            ]
          },
          {
            "description": "Nav Layer: H = Left",
            "manipulators": [
              {
                "type": "basic",
                "from": { "key_code": "h", "modifiers": { "optional": ["any"] } },
                "to": [{ "key_code": "left_arrow" }],
                "conditions": [{ "type": "variable_if", "name": "nav_layer", "value": 1 }]
              }
            ]
          },
          {
            "description": "Nav Layer: J = Down",
            "manipulators": [
              {
                "type": "basic",
                "from": { "key_code": "j", "modifiers": { "optional": ["any"] } },
                "to": [{ "key_code": "down_arrow" }],
                "conditions": [{ "type": "variable_if", "name": "nav_layer", "value": 1 }]
              }
            ]
          },
          {
            "description": "Nav Layer: K = Up",
            "manipulators": [
              {
                "type": "basic",
                "from": { "key_code": "k", "modifiers": { "optional": ["any"] } },
                "to": [{ "key_code": "up_arrow" }],
                "conditions": [{ "type": "variable_if", "name": "nav_layer", "value": 1 }]
              }
            ]
          },
          {
            "description": "Nav Layer: L = Right",
            "manipulators": [
              {
                "type": "basic",
                "from": { "key_code": "l", "modifiers": { "optional": ["any"] } },
                "to": [{ "key_code": "right_arrow" }],
                "conditions": [{ "type": "variable_if", "name": "nav_layer", "value": 1 }]
              }
            ]
          },
          {
            "description": "Home Row Mods - Left Hand",
            "manipulators": [
              {
                "type": "basic",
                "from": { "key_code": "a", "modifiers": { "optional": ["any"] } },
                "to_if_alone": [{ "key_code": "a" }],
                "to_if_held_down": [{ "key_code": "left_option" }],
                "parameters": { "basic.to_if_held_down_threshold_milliseconds": 300 }
              },
              {
                "type": "basic",
                "from": { "key_code": "s", "modifiers": { "optional": ["any"] } },
                "to_if_alone": [{ "key_code": "r" }],
                "to_if_held_down": [{ "key_code": "left_command" }],
                "parameters": { "basic.to_if_held_down_threshold_milliseconds": 300 }
              },
              {
                "type": "basic",
                "from": { "key_code": "d", "modifiers": { "optional": ["any"] } },
                "to_if_alone": [{ "key_code": "s" }],
                "to_if_held_down": [{ "key_code": "left_control" }],
                "parameters": { "basic.to_if_held_down_threshold_milliseconds": 300 }
              },
              {
                "type": "basic",
                "from": { "key_code": "f", "modifiers": { "optional": ["any"] } },
                "to_if_alone": [{ "key_code": "t" }],
                "to_if_held_down": [{ "key_code": "left_shift" }],
                "parameters": { "basic.to_if_held_down_threshold_milliseconds": 300 }
              }
            ]
          },
          {
            "description": "Home Row Mods - Right Hand",
            "manipulators": [
              {
                "type": "basic",
                "from": { "key_code": "j", "modifiers": { "optional": ["any"] } },
                "to_if_alone": [{ "key_code": "n" }],
                "to_if_held_down": [{ "key_code": "right_shift" }],
                "conditions": [{ "type": "variable_unless", "name": "nav_layer", "value": 1 }],
                "parameters": { "basic.to_if_held_down_threshold_milliseconds": 300 }
              },
              {
                "type": "basic",
                "from": { "key_code": "k", "modifiers": { "optional": ["any"] } },
                "to_if_alone": [{ "key_code": "e" }],
                "to_if_held_down": [{ "key_code": "right_control" }],
                "conditions": [{ "type": "variable_unless", "name": "nav_layer", "value": 1 }],
                "parameters": { "basic.to_if_held_down_threshold_milliseconds": 300 }
              },
              {
                "type": "basic",
                "from": { "key_code": "l", "modifiers": { "optional": ["any"] } },
                "to_if_alone": [{ "key_code": "i" }],
                "to_if_held_down": [{ "key_code": "right_command" }],
                "conditions": [{ "type": "variable_unless", "name": "nav_layer", "value": 1 }],
                "parameters": { "basic.to_if_held_down_threshold_milliseconds": 300 }
              },
              {
                "type": "basic",
                "from": { "key_code": "semicolon", "modifiers": { "optional": ["any"] } },
                "to_if_alone": [{ "key_code": "o" }],
                "to_if_held_down": [{ "key_code": "right_option" }],
                "parameters": { "basic.to_if_held_down_threshold_milliseconds": 300 }
              }
            ]
          }
        ]
      },
      "devices": [],
      "fn_function_keys": [],
      "virtual_hid_keyboard": {
        "country_code": 0,
        "indicate_sticky_modifier_keys_state": true,
        "mouse_key_xy_scale": 100
      }
    }
  ]
}

